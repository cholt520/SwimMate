<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="entity.Recommandation" table="recomandation">
   
      <id name="id" type="int" column="id">
         <generator class="native"/>
      </id>
      <property name="water_temp" column="water_temp" type="int"/>
      <property name="recommandation" column="recomandation" type="string"/>
   </class>
</hibernate-mapping>